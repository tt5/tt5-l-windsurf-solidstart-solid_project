{
  "name": "superstar",
  "version": "0.0.3",
  "type": "module",
  "bin": {
    "db": "./scripts/cli.ts"
  },
  "scripts": {
    "dev": "{ (lsof -t -i:3000) && { kill -9 $(lsof -t -i:3000); vinxi dev; } } || { vinxi dev; }",
    "dev:no-devuser": "VITE_DISABLE_DEV_USER=true vinxi dev",
    "build": "vinxi build",
    "postbuild": "npm run db:init",
    "start": "NODE_ENV=production node .output/server/index.mjs",
    "db:init": "mkdir -p ./data && npx tsx --no-warnings scripts/init-db.ts",
    "db:create-migration": "npx tsx --no-warnings scripts/create-migration.ts",
    "db:migrate": "npx tsx --no-warnings scripts/migrate.ts migrate",
    "db:check": "npx tsx --no-warnings scripts/check-db.ts",
    "db:verify": "npx tsx --no-warnings scripts/check-db.ts verify",
    "db:validate": "npx tsx --no-warnings scripts/validate-migrations.ts",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:e2e": "vitest run -c vitest.config.ts --mode e2e",
    "test:e2e:watch": "vitest watch -c vitest.config.ts --mode e2e",
    "test:e2e:ui": "vitest --ui -c vitest.config.ts --mode e2e",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "lint:css": "stylelint 'src/**/*.css' --fix",
    "lint:css:check": "stylelint 'src/**/*.css'",
    "lint:stylelint": "stylelint 'src/**/*.css'",
    "lint:stylelint:fix": "stylelint 'src/**/*.css' --fix",
    "db": "echo 'Use specific db commands: db:migrate, db:check, db:verify'"
  },
  "dependencies": {
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.3",
    "@solidjs/start": "^1.1.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pako": "^2.0.4",
    "@types/sqlite3": "^3.1.11",
    "jsonwebtoken": "^9.0.2",
    "pako": "^2.1.0",
    "solid-js": "^1.9.5",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "vinxi": "^0.5.7"
  },
  "engines": {
    "node": ">=22"
  },
  "devDependencies": {
    "@lightpanda/browser": "^1.0.1",
    "@playwright/test": "^1.55.1",
    "@solidjs/testing-library": "^0.8.4",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.6.2",
    "@types/yargs": "^17.0.33",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^23.2.0",
    "postcss": "^8.5.6",
    "postcss-scss": "^4.0.9",
    "puppeteer-core": "^24.23.0",
    "stylelint": "^16.24.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-order": "^7.0.0",
    "stylelint-scss": "^6.12.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
